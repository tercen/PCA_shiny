{
  "name": "PCA Shiny",
  "description": "Loads a web application to run and display a Principal Component Analysis.",
  "tags": [
    "dimensionality reduction",
    "multivariate statistics"
  ],
  "authors": [
    "tercen"
  ],
  "urls": [
    "https://github.com/tercen/PCA_shiny_operator"
  ],
  "isViewOnly": false,
  "properties": [
    {
      "kind": "EnumeratedProperty",
      "name": "Scale Spots",
      "defaultValue": "No",
      "description": "If the spots should be scaled",
      "values": [
        "No",
        "Yes"
      ]
    },
    {
      "kind": "DoubleProperty",
      "name": "Number of Components",
      "defaultValue": 5,
      "description": "number of components"
    },
    {
      "kind": "DoubleProperty",
      "name": "Subtract component",
      "defaultValue": 0,
      "description": "subtract component"
    }
  ]
}